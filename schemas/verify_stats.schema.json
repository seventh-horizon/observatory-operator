{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": ["timestamp", "certs", "ok", "fails", "elapsed"],
  "properties": {
    "timestamp": {
      "type": "string",
      "pattern": "^\\d{4}-\\d{2}-\\d{2}T.*Z$"
    },
    "certs": {
      "type": "integer",
      "minimum": 0
    },
    "ok": {
      "type": "integer",
      "minimum": 0
    },
    "fails": {
      "type": "integer",
      "minimum": 0
    },
    "elapsed": {
      "type": "integer",
      "minimum": 0
    },
    "req": {
      "type": ["integer", "string"]
    }
  },
  "additionalProperties": true
}
